public class main {
    public static void main(String[] args) {
        //siin on andmed pindala jaoks
        String alusString = "5.0"; 
        String korgusString = "3.0"; 

        //siin kasutan pindala arvutamise jaoks alamprogramme
        double roopkylikupindala = Pindalad.roopkylikS(alusString, korgusString);
        System.out.println("roopkuliku pindala: " + roopkylikupindala);

        double ristkylikupindala = Pindalad.ristkylikS(alusString, korgusString);
        System.out.println("ristkuliku pindala: " + ristkylikupindala);

        double kolmnurgapindala = Pindalad.kolmnurkS(alusString, korgusString);
        System.out.println("kolmnurga pindala: " + kolmnurgapindala);

        //siin kasutan ruumala arvutamise jaoks alamprogramme
        double risttahukaruumala = Ruumala.risttahukasV(5.0, 3.0, 2.0); 
        System.out.println("risttahuka ruumala: " + risttahukaruumala);

        double ristkulikprismaruumala = Ruumala.ristkulikprismaV(5.0, 3.0, 2.0);
        System.out.println("ristkulikprisma ruumala: " + ristkulikprismaruumala);
        
        double koonuseruumala = Ruumala.koonusV(2.0, 4.0); 
        System.out.println("koonuse ruumala: " + koonuseruumala);
    }
}
